<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.Dietapp.DeporteEjercicios.gluteos.Gluteos3">

    <Button
        android:id="@+id/button36"
        android:layout_width="133dp"
        android:layout_height="61dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/boton_volver"
        android:onClick="volverCategorias"
        android:text="@string/volverCategoD"
        app:layout_constraintEnd_toEndOf="@+id/completado"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/completado"
        app:layout_constraintTop_toBottomOf="@+id/completado" />

    <Button
        android:id="@+id/completado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="324dp"
        android:layout_marginEnd="48dp"
        android:text="Completado"
        android:onClick="completad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/medioej"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="316dp"
        android:text="Medio Ejercicio"
        android:onClick="mediocompletad"
        app:layout_constraintEnd_toStartOf="@+id/completado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>